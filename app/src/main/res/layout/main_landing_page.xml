<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/burger"
        android:layout_width="38dp"
        android:layout_height="38dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/border_radius"
        android:drawableStart="@drawable/baseline_table_rows_24"
        android:padding="7dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ImageButton
        android:id="@+id/profile_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/profile_button"
        android:src="@drawable/profile"
        android:padding="2dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/location_tracking"
        android:layout_width="327dp"
        android:layout_height="36dp"
        android:layout_marginStart="161dp"
        android:layout_marginEnd="161dp"
        android:background="#979797"
        android:drawableStart="@drawable/baseline_location_on_24"
        android:hint="@string/location_tracking"
        android:paddingStart="85dp"
        android:fontFamily="@font/oswald"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:textColorHint="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints,RtlSymmetry,TextContrastCheck,TouchTargetSizeCheck,VisualLintButtonSize"
        tools:layout_editor_absoluteY="265dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="285dp"
        android:layout_height="178dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="52dp"
        app:layout_constraintEnd_toStartOf="@+id/profile_button"
        app:layout_constraintHorizontal_bias="0.541"
        app:layout_constraintStart_toEndOf="@+id/burger"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img3"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/fire_button"
        android:layout_width="135dp"
        android:layout_height="122dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="49dp"
        android:background="@drawable/fire_border"
        android:padding="20dp"
        android:scaleType="fitXY"
        android:src="@drawable/firetruck"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/location_tracking"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck,ImageContrastCheck" />

    <ImageButton
        android:id="@+id/police_button"
        android:layout_width="135dp"
        android:layout_height="122dp"
        android:layout_marginTop="49dp"
        android:layout_marginEnd="63dp"
        android:background="@drawable/police_border"
        android:src="@drawable/police"
        android:padding="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/location_tracking"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/ambulance_button"
        android:layout_width="135dp"
        android:layout_height="122dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="56dp"
        android:background="@drawable/ambulance_border"
        android:drawableStart="@drawable/ambulance1"
        android:padding="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fire_button"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/coast_guard_button"
        android:layout_width="135dp"
        android:layout_height="122dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="63dp"
        android:drawableStart="@drawable/ambulance1"
        android:padding="20dp"
        android:background="@drawable/coast_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/police_button"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/oswald"
        android:letterSpacing="0.05"
        android:text="@string/fire_department"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fire_button" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="80dp"
        android:fontFamily="@font/oswald"
        android:text="@string/police_department"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/police_button" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="122dp"
        android:layout_height="42dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/oswald"
        android:text="@string/ambulance_department"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ambulance_button"
        tools:ignore="TextSizeCheck" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="125dp"
        android:layout_height="38dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="67dp"
        android:fontFamily="@font/oswald"
        android:text="@string/coast_guard_department"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/coast_guard_button"
        tools:ignore="TextSizeCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>