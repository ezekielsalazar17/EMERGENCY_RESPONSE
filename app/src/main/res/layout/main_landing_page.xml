<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/burger"
        android:layout_width="38dp"
        android:layout_height="38dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/border_radius"
        android:drawableStart="@drawable/baseline_table_rows_24"
        android:padding="7dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/profile_button"
        android:layout_width="36dp"
        android:layout_height="37dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/profile_button"
        android:src=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/location_tracking"
        android:layout_width="327dp"
        android:layout_height="36dp"
        android:layout_marginStart="161dp"
        android:layout_marginEnd="161dp"
        android:background="#979797"
        android:drawableStart="@drawable/baseline_location_on_24"
        android:hint="@string/location_tracking"
        android:paddingStart="85dp"
        android:fontFamily="@font/oswald"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:textColorHint="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints,RtlSymmetry,TextContrastCheck,TouchTargetSizeCheck,VisualLintButtonSize"
        tools:layout_editor_absoluteY="265dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="285dp"
        android:layout_height="178dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="52dp"
        app:layout_constraintEnd_toStartOf="@+id/profile_button"
        app:layout_constraintHorizontal_bias="0.541"
        app:layout_constraintStart_toEndOf="@+id/burger"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img3"
        tools:ignore="ContentDescription" />

    <Button
        android:id="@+id/fire_button"
        android:layout_width="135dp"
        android:layout_height="122dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="49dp"
        android:background="@drawable/fire_border"
        android:drawableStart="@drawable/firetruck"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/location_tracking"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/police_button"
        android:layout_width="135dp"
        android:layout_height="122dp"
        android:layout_marginTop="49dp"
        android:layout_marginEnd="63dp"
        android:background="@drawable/police_border"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/location_tracking"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/ambulance_button"
        android:layout_width="135dp"
        android:layout_height="122dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="56dp"
        android:background="@drawable/ambulance_border"
        android:drawableStart="@drawable/ambulance_copy"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fire_button"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/coast_guard_button"
        android:layout_width="135dp"
        android:layout_height="122dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="63dp"
        android:background="@drawable/coast_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/police_button"
        tools:ignore="SpeakableTextPresentCheck" />





</androidx.constraintlayout.widget.ConstraintLayout>